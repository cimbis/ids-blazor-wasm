{
  "name": "@ids-core/icons",
  "version": "14.8.6",
  "description": "Icons help the user find a desired action or piece of information relevant to their task. Icons can be used to mark a type of notification, status or validation, an action link or button, a type of insurance or sometimes (but rarely) an action button.",
  "main": "./dist/icons.css",
  "scripts": {
    "build": "rimraf ./dist ./*.zip && npm run generate && gulp -L",
    "generate": "npm run generate:variables && npm run generate:token-documentation && npm run generate:nucleo",
    "generate:variables": "node ./scripts/generate-variables.js",
    "generate:token-documentation": "npm run generate:raw-tokens && rimraf ./docs/components/utils/tokens.md && node ./scripts/generate-token-documentation.js",
    "generate:raw-tokens": "rimraf ./src/tokens/icons.json && node ./scripts/create-token-json.js",
    "generate:nucleo": "node ./scripts/export-to-nucleo.js",
    "build:prod": "npm run generate && npm test && rimraf ./dist ./*.zip && gulp build:prod -L",
    "build:ci": "npm run generate && rimraf ./dist ./*.zip && gulp build:prod -L",
    "test:prettify": "prettier --config .prettierrc ./dist/**/*.min.css --write && prettier --config .prettierrc ./dist/*.min.css --write",
    "test": "gulp pretest -L && npm run test:prettify && mocha test/diff.js"
  },
  "author": {
    "name": "If P&C Insurance"
  },
  "files": [
    "CHANGELOG.md",
    "/dist",
    "!/dist/*.zip",
    "/docs",
    "/demo",
    "/src"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=16.13.1",
    "npm": ">=8.2.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "cssnano": "^4.1.10",
    "diff": "^4.0.2",
    "glob": "^7.1.2",
    "gulp": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-zip": "^4.2.0",
    "humanize-string": "^2.1.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "style-dictionary": "^3.1.0",
    "stylint-stylish": "^2.0.0",
    "svgo": "^1.3.0"
  },
  "keywords": [
    "components-library",
    "design-system",
    "icons",
    "product",
    "coverage",
    "flag",
    "products",
    "coverages",
    "flags",
    "some",
    "social-media",
    "symbol",
    "ui"
  ],
