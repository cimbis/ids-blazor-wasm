{
  "name": "@ids-core/core",
  "version": "14.29.10",
  "description": "Core functionality",
  "main": "./dist/core.css",
  "scripts": {
    "build": "rimraf ./dist ./*.zip && yarn run generate && gulp -L",
    "build:prod": "yarn test && rimraf ./dist ./*.zip && yarn run generate && gulp build:prod -L",
    "build:ci": "rimraf ./dist ./*.zip && yarn run generate && gulp build:prod -L",
    "generate:variables": "node ./scripts/generate-variables.js",
    "generate:token-documentation": "node ./scripts/generate-token-documentation.js",
    "generate": "yarn run generate:variables && yarn run generate:token-documentation",
    "test:prettify": "prettier --config .prettierrc ./dist/**/*.min.css --write",
    "test": "gulp pretest -L && yarn run test:prettify && yarn run mocha test/diff.js"
  },
  "keywords": [
    "components-library",
    "design-system",
    "core"
  ],
  "author": {
    "name": "If P&C Insurance"
  },
  "files": [
